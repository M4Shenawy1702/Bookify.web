{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/lib/bootbox.js/"
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css",
        "animate.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "sweetalert2@11.12.2",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "css/bootstrap.css.map",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "js/bootstrap.bundle.js.map",
        "js/bootstrap.min.js.map"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png",
        "css/dataTables.bootstrap4.min.css",
        "css/dataTables.bootstrap4.css",
        "js/dataTables.bootstrap4.js",
        "js/dataTables.bootstrap4.min.js"
      ]
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "js/select2.min.js",
        "css/select2.min.css"
      ]
    },
    {
      "library": "bootstrap-daterangepicker@3.0.5",
      "destination": "wwwroot/lib/bootstrap-daterangepicker/",
      "files": [
        "daterangepicker.min.js",
        "daterangepicker.min.css",
        "moment.min.js"
      ]
    },
    {
      "library": "tinymce@7.6.0",
      "destination": "wwwroot/lib/tinymce/"
    },
    {
      "library": "expressive-annotations-validate@2.7.0",
      "destination": "wwwroot/lib/expressive-annotations-validate/"
    }
